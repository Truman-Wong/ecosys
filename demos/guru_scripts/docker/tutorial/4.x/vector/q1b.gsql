#enter the graph
USE GRAPH financialGraph

CREATE OR REPLACE QUERY q1b () SYNTAX v3 {

  v = SELECT a
      FROM (a:Account)
      ORDER BY gds.vector.norm(a.emb1, "L2")
      LIMIT 10;

  PRINT v WITH VECTOR;

  v = SELECT a
      FROM (a:v)
      WHERE a.name == "Scott";

  PRINT v WITH VECTOR;

}

#compile and install the query as a stored procedure
install query -single q1b

#run the query
run query q1b()
